{
  "name": "vocal",
  "version": "0.0.1-alpha",
  "description": "Vocaloid-gui is a simple voice changer",
  "main": "./index.js",
  "scripts": {
    "start": "npm run dev:main & npm run dev:renderer",
    "dev:main": "cross-env NODE_ENV=dev electron .",
    "dev:renderer": "cross-env NODE_ENV=dev PLATFORM=electron webpack-dev-server --entry ./src/index.tsx --config ./build/webpack.config.base.js",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --entry ./src/index.tsx --config ./build/webpack.config.base.js",
    "build:web": "cross-env NODE_ENV=production webpack --entry ./src/index.tsx --config ./build/webpack.config.base.js",
    "build:renderer": "cross-env NODE_ENV=production PLATFORM=electron webpack --entry ./src/index.tsx --config ./build/webpack.config.base.js",
    "build:main": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.electron.main.js",
    "build:desktop": "npm run build:renderer && npm run build:main",
    "build:all": "npm run build:web && npm run build:desktop",
    "pack:win": "cd ./dist/electron && electron-packager ./ vocal --win --asar --out ../vocal --icon=./renderer/logo/favicon.icon --arch=x64 --app-version=0.0.1-alpha --overwrite",
    "pack:mac": "cd ./dist/electron && electron-packager ./ vocal --platform=darwin --asar --out ../vocal --./icon=renderer/logo/favicon.png --arch=x64 --app-version=0.0.1-alpha --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elgine/vocaloid-gui.git"
  },
  "keywords": [
    "cross-platfofrm",
    "electron",
    "typescript",
    "audio editor",
    "voice changer"
  ],
  "author": "elgine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elgine/vocaloid-gui/issues"
  },
  "homepage": "https://github.com/elgine/vocaloid-gui#readme",
